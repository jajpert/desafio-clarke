{
  "semi": true,
  "trailingComma": "none",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "endOfLine": "crlf",
  "no-multiple-empty-lines": ["error", { "max": 1 }],
  "eol-last": ["error", "always"],
  "json-prettyprint/indent": ["error", 2],
  "no-trailing-spaces": ["error", { "skipBlankLines": true }],
  "space-infix-ops": "error"
}
